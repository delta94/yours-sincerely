{
  "name": "yours-sincerely",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://yours-sincerely.org",
  "scripts": {
    "start": "sh -ac '. ./.env.dev; firebase use dev && react-scripts start'",
    "build": "react-scripts build",
    "deploy:prod": "sh -ac '. ./.env.prod; firebase use prod && npm run build && firebase deploy'",
    "deploy:dev": "sh -ac '. ./.env.dev; firebase use dev && npm run build && firebase deploy'",
    "deploy:functions": "sh -ac '. ./.env.prod; firebase use prod && firebase deploy --only functions'",
    "test": "echo \"No tests \" && exit 0",
    "format": "prettier . --write",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "date-fns": "^2.15.0",
    "firebase": "^7.17.1",
    "lodash": "^4.17.19",
    "raw.macro": "^0.4.1",
    "react": "^16.13.1",
    "react-content-loader": "^5.1.0",
    "react-dom": "^16.13.1",
    "react-firebase-hooks": "^2.2.0",
    "react-helmet": "^6.1.0",
    "react-instantsearch-dom": "^6.7.0",
    "react-modal": "^3.11.2",
    "react-router-dom": "^5.2.0",
    "react-tooltip": "^4.2.7",
    "snyk": "^1.369.1",
    "styled-components": "^5.1.1",
    "styled-normalize": "^8.0.7"
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react-scripts": "^3.4.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "snyk": true
}
