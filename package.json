{
  "name": "yours-sincerely",
  "description": "To write is to put pen to paper or fingers to keyboard. If you have trouble falling asleep, write down all the thoughts swimming around your head before getting into bed as a way to clear your mind.",
  "author": "Kaiyu Hsu <im.kaiyu@gmail.com> (http://github.com/tehkaiyu)",
  "license": "MIT",
  "version": "0.0.0",
  "main": "server/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/its-bananas/yours-sincerely"
  },
  "engines": {
    "node": "10.15.3",
    "npm": "6.9.0"
  },
  "scripts": {
    "setup": "npm i && next run build && cp ./config/.env.example ./config/.env.local",
    "build": "next build client",
    "start": "NODE_ENV=production node server",
    "dev": "env-cmd -f ./config/.env.local node server/index.js --ext js,graphql",
    "dev:storybook": "start-storybook -p 5010 -s ./client",
    "deploy": "node tools/deploy",
    "deploy:storybook": "storybook-to-ghpages",
    "lint": "eslint --ext .js --env browser,node client/ server/",
    "lint:fix": "eslint --fix --ext .js --env browser,node client/ server",
    "create:components": "node ./tools/components/create-components.js",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "_moduleAliases": {
    "@server": "./server",
    "@client": "./client",
    "@components": "./client/components"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.2",
    "@apollo/react-ssr": "^3.1.2",
    "@material-ui/core": "^4.5.0",
    "@material-ui/icons": "^4.4.3",
    "@material-ui/styles": "^4.5.0",
    "apollo-boost": "^0.4.4",
    "apollo-utilities": "^1.3.2",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "formik": "^1.5.8",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "helmet": "^3.21.1",
    "isomorphic-unfetch": "^3.0.0",
    "module-alias": "^2.2.2",
    "next": "^9.1.1",
    "next-offline": "^4.0.6",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-adopt": "^0.6.0",
    "react-content-loader": "^4.3.1",
    "react-dom": "^16.10.2",
    "react-swipeable-views": "^0.13.3",
    "react-transition-group": "^4.3.0",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@sambego/storybook-state": "^1.3.6",
    "@storybook/addon-a11y": "^5.2.3",
    "@storybook/addon-actions": "^5.2.3",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-knobs": "^5.2.3",
    "@storybook/addon-links": "^5.2.3",
    "@storybook/addon-notes": "^5.2.3",
    "@storybook/addon-storysource": "^5.2.3",
    "@storybook/addon-viewport": "^5.2.3",
    "@storybook/react": "^5.2.3",
    "@storybook/storybook-deployer": "^2.8.1",
    "babel-eslint": "^10.0.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "bluebird": "^3.7.0",
    "env-cmd": "^10.0.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "fs-extra": "^8.1.0",
    "husky": "^3.0.8",
    "inquirer": "^7.0.0",
    "jest": "^24.9.0",
    "lodash": "^4.17.15",
    "prettier": "^1.18.2",
