{
  "name": "yours-sincerely",
  "description": "To write is to put pen to paper or fingers to keyboard. If you have trouble falling asleep, write down all the thoughts swimming around your head before getting into bed as a way to clear your mind.",
  "author": "Kaiyu Hsu <im.kaiyu@gmail.com> (http://github.com/tehkaiyu)",
  "license": "MIT",
  "version": "0.0.0",
  "main": "server/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/its-bananas/yours-sincerely"
  },
  "engines": {
    "node": "10.15.3",
    "npm": "6.9.0"
  },
  "scripts": {
    "setup": "npm i && next run build && cp ./config/.env.example ./config/.env.local",
    "build": "next build client",
    "start": "node --max_old_space_size=460 server",
    "dev": "env-cmd -f ./config/.env.local nodemon server/index.js --ext js,graphql",
    "dev:storybook": "start-storybook -p 5010 -s ./client",
    "dev:debug": "env-cmd -f ./config/.env.local ndb node server/index.js",
    "deploy": "node tools/deploy",
    "deploy:schema": "prisma deploy -e ./config/.env.local",
    "deploy:storybook": "storybook-to-ghpages",
    "lint": "eslint --ext .js --env browser,node client/ server/",
    "lint:fix": "eslint --fix --ext .js --env browser,node client/ server",
    "create:components": "node ./tools/components/create-components.js",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "_moduleAliases": {
    "@server": "./server",
    "@client": "./client",
    "@components": "./client/components"
  },
  "dependencies": {
    "@material-ui/core": "^4.3.0",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.3.0",
    "adorable-avatars": "^0.4.1",
    "apollo-boost": "^0.4.3",
    "apollo-server": "^2.8.0",
    "apollo-server-express": "^2.8.0",
    "aws-sdk": "^2.503.0",
    "bcrypt": "^3.0.6",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "formik": "^1.5.8",
    "graphql": "^14.4.2",
    "graphql-import": "^0.7.1",
    "graphql-middleware": "^3.0.3",
    "graphql-shield": "^6.0.4",
    "graphql-tag": "^2.10.1",
    "helmet": "^3.20.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.1",
    "moniker": "^0.1.2",
    "next": "^9.0.2",
    "next-offline": "^4.0.2",
    "next-with-apollo": "^4.1.1",
    "nprogress": "^0.2.0",
    "prisma-binding": "^2.3.15",
    "prisma-client-lib": "^1.34.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-adopt": "^0.6.0",
    "react-apollo": "^2.5.8",
    "react-content-loader": "^4.2.2",
    "react-dom": "^16.8.6",
    "react-swipeable-views": "^0.13.3",
    "react-transition-group": "^4.2.1",
    "uuid": "^3.3.2",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@sambego/storybook-state": "^1.3.6",
    "@storybook/addon-a11y": "^5.1.9",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addon-notes": "^5.1.9",
    "@storybook/addon-storysource": "^5.1.9",
    "@storybook/addon-viewport": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "@storybook/storybook-deployer": "^2.8.1",
    "babel-eslint": "^10.0.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "bluebird": "^3.5.5",
    "env-cmd": "^9.0.3",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "fs-extra": "^8.1.0",
    "husky": "^3.0.2",
    "inquirer": "^6.5.0",
    "jest": "^24.8.0",
    "lodash": "^4.17.15",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react-test-renderer": "^16.8.6"
  }
}
